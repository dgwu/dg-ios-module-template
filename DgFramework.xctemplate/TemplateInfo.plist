            <string>Configurator</string>
            <key>Path</key>
            <string>Configurator/___PACKAGENAME___Configurator.swift</string>
        </dict>
        <key>Constant/___PACKAGENAME___Constants.swift</key>
        <dict>
            <key>Group</key>
            <string>Constant</string>
