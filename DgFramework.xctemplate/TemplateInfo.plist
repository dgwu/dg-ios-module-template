            <string>Scene</string>
            <key>Path</key>
            <string>Placeholder.swift</string>
        </dict>
        <key>Wireframe/___PACKAGENAME___Wireframe.swift</key>
        <dict>
            <key>Group</key>
            <string>Wireframe</string>
            <key>Path</key>
            <string>Wireframe/___PACKAGENAME___Wireframe.swift</string>
        </dict>
        <key>Assets.xcassets</key>
        <dict>
            <key>Path</key>
            <string>Assets.xcassets</string>
        </dict>
